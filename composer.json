{
    "name": "sasa1007/google-captcha-bundle",
    "description": "Google reCAPTCHA bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "bundle", "captcha", "recaptcha", "google"],
    "homepage": "https://github.com/sasa1007/googleCaptchaBundle",
    "authors": [
        {
            "name": "Sasa Milivojevic",
            "email": "sasam09@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0|^7.0|^8.0",
        "symfony/dependency-injection": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BeckUp\\GoogleCaptchaBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0|7.0|8.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 